{
    "name": "omersia/gdpr",
    "description": "GDPR compliance package for Omersia - Cookie consent, DSAR, and data deletion",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0",
        "illuminate/database": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Omersia\\Gdpr\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omersia\\Gdpr\\GdprServiceProvider"
            ]
        }
    }
}
